<config version='1.0'>
   
   <!-- scope it to mrcs_folder... -->
   <scope type='m_mrcs_folder'>
      
      <!-- the document list component definition -->
      <component id="newdoccontainer">         
         <!-- Component Contract -->
         <params>
            <param name="objectId" required="true"></param>
         </params>                  
         
         <!-- Component Layout -->
         <pages>
            <start>/mrcs/pages/MrcsNewDocument.jsp</start>
            <!--  custom component pages must be specified here -->
            <UserSpecified>/mrcs/pages/MrcsNewDocumentCustomComponent.jsp</UserSpecified>
         </pages>
         
         <!-- Component Behavior -->
         <class>com.medtronic.documentum.mrcs.client.MrcsNewDocumentComponentMKII</class>
         <nlsbundle>com.medtronic.documentum.mrcs.client.MrcsNewDocumentContainerMKIIStrings</nlsbundle>
                  
      </component>
      
   </scope>
</config>

