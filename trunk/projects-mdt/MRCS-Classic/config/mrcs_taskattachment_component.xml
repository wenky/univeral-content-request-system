<config version='1.0'>
   <scope>

      <component id="taskattachment" extends="taskattachment:webcomponent/config/library/workflow/attachment/taskattachment_component.xml">

         <!-- Description (not NLS'd) -->
         <desc>
            This component is part of the task manager component. It manages the
            attachment information during task management (viewing inbox tasks).
         </desc>

         <!-- component contract -->
         <params>
            <param name="objectId" required="true"/>
         </params>

         <pages>
            <start>/webcomponent/library/workflow/attachment/taskAttachmentList.jsp</start>
            <list>/webcomponent/library/workflow/attachment/taskAttachmentList.jsp</list>
            <drilldown>/mrcs/pages/MrcsTaskAttachmentDrilldown.jsp</drilldown>
         </pages>

         <class>com.documentum.webcomponent.library.workflow.attachment.TaskAttachment</class>
        <nlsbundle>com.documentum.webcomponent.library.workflow.attachment.AttachmentNlsProp</nlsbundle>

         <helpcontextid>taskattachment</helpcontextid>
      </component>


   </scope>


</config>

