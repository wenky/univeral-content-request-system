<config version='1.0'>
   
   <!-- scope it to mrcs_folder... -->
   <scope type='m_mrcs_folder'>
      
      <!-- the document list component definition -->
      <component id="newdoc_custom_component_sample">         
         <!-- Component Contract -->
         <params>
            <param name="objectId" required="true"></param>
            <param name="docstate" required="true"></param>
         </params>                  
         
         <!-- Component Layout -->
         <pages>
            <start>/mrcs/pages/MrcsNewDocumentCustomComponent.jsp</start>
         </pages>
         
         <!-- Component Behavior -->
         <class>com.medtronic.documentum.mrcs.client.MrcsNewDocumentCustomComponent</class>
         <nlsbundle>com.medtronic.documentum.mrcs.client.MrcsNewDocumentCustomComponentNlsProp</nlsbundle>
                  
      </component>
      
   </scope>
</config>

