<config version='1.0'>
   <scope type="m_mrcs_central_document">

      <!-- docbaseattributelist definition -->
      <attributelist id="attributes" extends="attributes:mrcs/config/mrcs_attributes_control_document_config.xml">

         <data_dictionary_population>
            <enable>true</enable>
            <ddscopes>
               <ddscope name="application">webtop</ddscope>
            </ddscopes>
            <ignore_attributes></ignore_attributes>            
            <readonly_attributes>
               <attribute name="object_name"/>
               <attribute name="owner_name"/>
               <attribute name="r_version_label"/>
               <attribute name="m_mrcs_central_ref_number"/>
            </readonly_attributes>
         </data_dictionary_population>
         
      </attributelist>
   </scope>
   
</config>

