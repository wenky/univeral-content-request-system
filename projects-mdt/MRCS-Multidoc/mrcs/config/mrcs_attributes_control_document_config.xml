<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<config version='1.0'>
   <scope type="m_mrcs_document">

      <!-- docbaseattributelist definition -->
      <attributelist id="attributes" extends="attributes:webcomponent/config/library/attributes_dm_document_docbaseattributelist.xml">

         <data_dictionary_population>
            <enable>true</enable>
            <ddscopes>
               <ddscope name="application">webtop</ddscope>
            </ddscopes>
            <ignore_attributes></ignore_attributes>            
            <readonly_attributes>
               <attribute name="object_name"/>
               <attribute name="owner_name"/>
               <!--  CEM: by default, version label not editable for 4.1.2 compatibility, but this isn't
                          a hard and fast rule for 4.2, override in projects to enable as needed... -->
               <attribute name="r_version_label"/>
            </readonly_attributes>
         </data_dictionary_population>
         
      </attributelist>
   </scope>
   
</config>

