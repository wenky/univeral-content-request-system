<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<config version='1.0'>
   <scope type="m_mrcs_document">

      <!-- Checkin component definition -->
      <component id="schedulePromote" >

         <!-- Description (not NLS'd) -->
         <desc>
            You can use this component to allow users to shedule a promote for a 
            document to the date (and time?) specified.
         </desc>

         <!-- Component Contract -->
         <params>
            <param name="objectId" required="true"></param>
         </params>

         <!-- Component Layout -->
         <pages>
            <start>/mrcs/pages/schedulePromote.jsp</start>
          </pages>

         <!-- Component Behavior  <class>com.documentum.webcomponent.library.checkin.Checkin</class>-->
         <class>com.medtronic.documentum.mrcs.client.SchedulePromote</class>
         <nlsbundle>com.medtronic.documentum.mrcs.client.MrcsScheduledPromoteNlsProp</nlsbundle>

      </component>
   </scope>

</config>

