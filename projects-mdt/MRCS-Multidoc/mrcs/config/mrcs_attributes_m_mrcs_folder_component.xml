<config version='1.0'>

   <!-- dm_folder specific version -->
   <scope type='dm_folder'>
      <component id="attributes" extends="attributes:webcomponent/config/library/attributes/attributes_dm_sysobject_component.xml">
         
         <!-- Description (not NLS'd) -->
         <desc>
            Attribute component:  Provides UI and behaviour for showing and 
            editing a folder's attributes.
         </desc>

         <!-- Component Layouts -->
         <pages>
            <start>/webcomponent/library/attributes/attributes_dm_folder.jsp</start>
         </pages>

         <class>com.medtronic.documentum.mrcs.client.MrcsGeneralAttributes</class>

         
         <!-- showpagesastabs is used in order to indicate whether  -->
         <!-- each category in DocbaseAttributeList should be shown -->
         <!-- as tabs or not (default to be false)                  -->
         <showpagesastabs>true</showpagesastabs>

         <helpcontextid>attributesdmfolder</helpcontextid>
      </component>
   </scope>
   
</config>

