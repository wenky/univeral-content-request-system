<!-- File           mrcsnewgroupingfolder_component.xml                    -->
<!-- Description    collects information necessary to creating a new MRCS  -->
<!--                grouping folder (the gf type has been decided...)      -->
<!-- Created on     February 2005                                          -->
<!-- Tab width      3                                                      -->
<!--                                                                       -->
<!--***********************************************************************-->

<config version='1.0'>
   <scope>
      
      <!-- component definition -->
      <component id="mrcsnewgroupingfolder_custom">
         
         <!-- Component Contract -->
         <params>
            <!-- objectId provides id of folder for import -->
            <param name="objectId" required="true"></param>
            <param name="folderType" required="true"></param>
            <param name="mrcsApplication" required="true"></param>
         </params>
         
         <!-- Component Layout -->
         <pages>
            <start>/mrcs/pages/MrcsNewGroupingFolderCustomComponent.jsp</start>
         </pages>
         
         <!-- Component Behavior -->
         <class>com.medtronic.documentum.mrcs.client.MrcsNewGroupingFolderCustomComponent</class>
         <nlsbundle>com.medtronic.documentum.mrcs.client.MrcsNewGroupingFolderStrings</nlsbundle>
                       
      </component>
      
   </scope>
</config>

