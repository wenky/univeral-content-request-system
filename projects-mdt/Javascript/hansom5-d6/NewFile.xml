                    </MrcsWorkflowTask>
                    <MrcsWorkflowTask>
                        <Name>Contract Approval Complete</Name>
                        <MethodConfiguration>
                            <entry>
                                <string>ServerPlugins</string>
                                <list>
                                    <MrcsPlugin>
                                        <PluginClassName>com.medtronic.documentum.mrcs.server.plugin.QueueEvent</PluginClassName>
                                        <PluginConfiguration>
                                            <entry>
                                                <string>EventName</string>
                                                <string>Contract Approval Complete</string>
                                            </entry>
                                            <entry>
                                                <string>Message</string>
                                                <string>Your CRDM Supply Chain contract has been successfully approved!</string>
                                            </entry>
                                        </PluginConfiguration>
                                    </MrcsPlugin>
                                    <!--  schedule an expiration date -->
                                </list>
                            </entry>
                        </MethodConfiguration>
                    </MrcsWorkflowTask>
                    <MrcsWorkflowTask>
                        <Name>Contract Rejected</Name>
                    </MrcsWorkflowTask>
                </Tasks>
            </MrcsWorkflow>
        </entry>
    </MrcsWorkflows>

    <ESignature>
        <NoOfSigns>20</NoOfSigns>
        <SigningReasonsQuery>SELECT mrcs_property_value FROM m_mrcs_value_assistance WHERE mrcs_object_type = 'm_mrcs_crmsdo_document' and mrcs_property_name = 'signing_reasons' order by 1 asc</SigningReasonsQuery>
    </ESignature>

